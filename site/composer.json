{
  "name": "roots/bedrock",
  "type": "project",
  "license": "MIT",
  "description": "WordPress boilerplate with modern development tools, easier configuration, and an improved folder structure",
  "homepage": "https://roots.io/bedrock/",
  "authors": [
    {
      "name": "Scott Walkinshaw",
      "email": "scott.walkinshaw@gmail.com",
      "homepage": "https://github.com/swalkinshaw"
    },
    {
      "name": "Ben Word",
      "email": "ben@benword.com",
      "homepage": "https://github.com/retlehs"
    }
  ],
  "keywords": [
    "bedrock", "roots", "wordpress", "stack", "composer", "vagrant", "wp"
  ],
  "support": {
    "issues": "https://github.com/roots/bedrock/issues",
    "forum": "https://discourse.roots.io/category/bedrock"
  },
  "config": {
    "preferred-install": "dist"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
        "type": "vcs",
        "url": "https://github.com/getdave/wp-intervention"
    },
    {
      "type": "package",
      "package": {
        "name": "advanced-custom-fields/advanced-custom-fields-pro",
        "version": "5.3.9.2",
        "type": "wordpress-plugin",
        "dist": {
          "type": "zip",
          "url": "https://connect.advancedcustomfields.com/index.php?p=pro&a=download&k=b3JkZXJfaWQ9NjU4NjN8dHlwZT1kZXZlbG9wZXJ8ZGF0ZT0yMDE1LTEwLTA2IDE1OjA1OjI2"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "deliciousbrains/wp-migrate-db-pro",
        "type": "wordpress-plugin",
        "version": "1.8.1",
        "dist": {
            "type": "zip",
            "url": "https://deliciousbrains.com/dl/wp-migrate-db-pro-1.8.1.zip?licence_key=41a27671-816b-4600-b80d-782969ef106b&site_url=buzzeasy.test"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "deliciousbrains/wp-migrate-db-pro-media-files",
        "type": "wordpress-plugin",
        "version": "1.4.9",
        "dist": {
            "type": "zip",
            "url": "https://deliciousbrains.com/dl/wp-migrate-db-pro-media-files-latest.zip?licence_key=41a27671-816b-4600-b80d-782969ef106b&site_url=buzzeasy.test"
        }
      }
    }
  ],
  "require": {
    "php": "^7.0",
    "composer/installers": "~1.0.12",
    "vlucas/phpdotenv": "^2.0.1",
    "oscarotero/env": "^1.0",
    "roots/wp-password-bcrypt": "^1.0.0",
    "wpackagist-plugin/akismet": "^3.2",
    "wpackagist-plugin/duplicate-post": "^3.1",
    "wpackagist-plugin/disable-comments": "^1.6",
    "wpackagist-plugin/stream": "^3.2",
    "wpackagist-plugin/rewrite-rules-inspector": "^1.2.1",
    "advanced-custom-fields/advanced-custom-fields-pro": "*",
    "wpackagist-plugin/posts-to-posts": "~1.6.5",
    "wpackagist-plugin/user-switching": "^1.0.9",
    "wpackagist-plugin/ewww-image-optimizer": "~4.1.0",
    "wpackagist-plugin/members": "~1.1.3",
    "deliciousbrains/wp-migrate-db-pro": "1.8.1",
    "deliciousbrains/wp-migrate-db-pro-media-files": "1.4.9",
    "wpackagist-plugin/breadcrumb-trail": "~1.0.0",
    "wpackagist-plugin/location-nav-menu-for-acf": "~1.1.1",
    "wpackagist-plugin/better-wp-security": "~6.2.0",
    "wpackagist-plugin/enhanced-media-library": "^2.2.4",
    "wpackagist-plugin/imsanity": "~2.3.8",
    "wpackagist-plugin/disable-users": "^1.0",
    "wpackagist-plugin/nofollow": "^1.4.5",
    "wpackagist-plugin/stop-user-enumeration": "^1.3",
    "wpackagist-plugin/post-smtp": "^1.8.1",
    "johnpbloch/wordpress": "^4.8.2",
    "getdave/wp-intervention": "^1.1.0",
    "jjgrainger/wp-custom-post-type-class": "dev-master",
    "wpackagist-plugin/redirection": "^3.2",
    "wpackagist-plugin/autodescription": "^3.0",
    "rollbar/rollbar": "~1.1"    
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.0.2"
  },
  "extra": {
    "installer-paths": {
      "web/app/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "web/app/plugins/{$name}/": ["type:wordpress-plugin"],
      "web/app/themes/{$name}/": ["type:wordpress-theme"]
    },
    "wordpress-install-dir": "web/wp"
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ],
    "test": [
      "vendor/bin/phpcs"
    ]
  }
}
